{
    "manifest_version": 3,
    "name": "Quick Notes",
    "version": "1.0",
    "description": "A simple Chrome extension to jot down quick notes.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": [
      "storage"
    ]
  }
  